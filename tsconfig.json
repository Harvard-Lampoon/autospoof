{
    "compilerOptions": {
	"target": "es2021",
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true,
	"module": "commonjs",
	"strict": true,
	"noImplicitAny": true,
	"noImplicitThis": true,
	"noUnusedLocals": true,
	"noUnusedParameters": true,
	"noImplicitReturns": true,
	"noFallthroughCasesInSwitch": true,
	"noImplicitOverride": true,
	"noPropertyAccessFromIndexSignature": true
    }
}
